{
  "name": "mirror",
  "version": "0.1.0",
  "private": true,
  "author": "demay",
  "description": "Mirror app",
  "main": "./public/electron.js",
  "email": "d.mayer97@web.de",
  "homepage": "./",
  "dependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@mui/icons-material": "^5.0.1",
    "@mui/material": "^5.0.0",
    "@mui/utils": "^5.0.0",
    "@reduxjs/toolkit": "^1.8.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/fabric": "4.2.4",
    "chart.js": "^3.5.1",
    "concurrently": "^8.2.2",
    "fabric": "4.3.1",
    "luxon": "^3.4.3",
    "react": "^17.0.2",
    "react-calendar": "3.3.1",
    "react-chartjs-2": "^3.0.5",
    "react-color": "^2.19.3",
    "react-dom": "^17.0.2",
    "react-open-weather": "^1.1.5",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.3.0",
    "react-router-transition": "^2.1.0",
    "react-scripts": "4.0.3",
    "serve": "^14.2.1",
    "typescript": "^4.3.5",
    "unstated-next": "^1.1.0",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "go": "concurrently \"npm run start\" \"npm run proxy\"",
    "start": "set port=8080 && react-scripts start",
    "build": "react-scripts build",
    "proxy": "lcp --proxyUrl https://efa.avv-augsburg.de",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "serve": "serve -s build",
    "lint": "eslint **/*.js **/*.tsx",
    "fix": "eslint --fix **/*.js **/*.tsx",
    "electron:start": "concurrently -k \"cross-env BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electronmon .\"",
    "electron:package:mac": "electron-builder -m -c.extraMetadata.main=build/electron.js",
    "electron:package:win": "electron-builder -w -c.extraMetadata.main=build/electron.js",
    "electron:package:linux": "electron-builder -l -c.extraMetadata.main=build/electron.js"
  },
  "browserslist": {
    "production": [
      "last 1 electron version",
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 electron version",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/luxon": "^3.3.3",
    "@types/react": "^17.0.19",
    "@types/react-calendar": "^3.4.3",
    "@types/react-color": "^3.0.6",
    "@types/react-router-dom": "^5.1.9",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "cross-env": "^7.0.3",
    "electron": "^27.0.2",
    "electron-builder": "^24.6.4",
    "electronmon": "^2.0.2",
    "eslint-plugin-react": "^7.29.3",
    "react-router-dom": "^5.3.0",
    "wait-on": "^7.0.1"
  },
  "build": {
    "appId": "com.electron.mirror",
    "productName": "Mirror App",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "public"
    },
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "deb"
    }
  }
}
